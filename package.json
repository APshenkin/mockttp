    "testing",
    "test",
    "mock",
    "stub",
    "fake",
    "http",
    "server"
  ],
  "author": "Tim Perry <pimterry@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pimterry/http-server-mock/issues"
  },
  "homepage": "https://github.com/pimterry/http-server-mock#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "request": "^2.75.0",
    "request-promise-native": "^1.0.3",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "typescript": "^2.1.0-dev.20161018",
    "typings": "^1.3.3"
  },
  "engines": {
    "node": ">=4.0.0"
